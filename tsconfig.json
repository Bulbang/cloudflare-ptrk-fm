{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "CommonJS",
        "target": "ES2020",
        "lib": ["ES2020"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": [
            "node",
            "@cloudflare/workers-types",
            "@types/service-worker-mock"
        ]
    }
}
